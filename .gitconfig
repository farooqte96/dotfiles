[core]
    editor = vim
[push]
    default = simple
[alias]
    br      = branch
    ci      = commit
    cia     = commit --amend
    co      = checkout
    cp      = cherry-pick
    hist    = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    rb      = rebase
    rbi     = rebase --interactive
    pr      = pull --rebase
